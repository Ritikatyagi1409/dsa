public class Running
{
    static void runningsum(int arr[])
    {
        for(int i=1;i<arr.length;i++)
        {
             arr[i]=arr[i]+arr[i-1];
        }
        for(int i=0;i<arr.length;i++){
        System.out.println (arr[i]);
        }
    }
    public static void main(String[] args)
    {
        int arr[]={2,3,4,5,1,6,3};
        runningsum(arr);
    }
}
